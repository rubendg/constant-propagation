if (x === 100)
{
  y = 100;
}
else {
       if (x === 101)
       {
         y = 101;
         x = 100;
         while (x < 1000)
         {
           {
             y = 5;
           };
           x = x + 1;
         };
         y = 102;
         y = 103;
       }
       else {
              if (x === 102)
              {
                x = 100;
                while (x < 1000)
                {
                  {
                    y = 5;
                  };
                  x = x + 1;
                };
                y = 102;
                y = 103;
              }
              else {
                     {
                       y = 103;
                     };
                   };
            };
     };